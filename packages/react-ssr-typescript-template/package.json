{
  "name": "@laffery/webpack-react-ssr-ts",
  "version": "0.3.8",
  "author": "Laffery",
  "description": "template for webpack + react + ssr + typescript",
  "private": false,
  "keywords": [
    "webpack",
    "react",
    "typescript",
    "ssr"
  ],
  "types": "src/typings/*",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently -k -p name -n \"client,server,server\" -c \"yellow,green,blue\" \"yarn dev:client\" \"yarn dev:server:webpack\" \"yarn dev:server:nodemon\"",
    "start": "cross-env NODE_ENV=production nodemon dist/server/index.js",
    "build": "cross-env NODE_ENV=production yarn build:client && yarn build:server",
    "clean:build": "rm -rf dist",
    "clean": "rm -rf dist && rm -rf node_modules",
    "test": "echo NO TEST MODULE FOUND",
    "dev:client": "webpack -c webpack/webpack.dev.js -w",
    "dev:server:webpack": "webpack -c webpack/webpack.server.js -w",
    "dev:server:nodemon": "wait-on -l dist/server/index.js && yarn start",
    "build:client": "rm -rf dist/client && webpack build -c webpack/webpack.prod.js",
    "build:server": "rm -rf dist/server && webpack build -c webpack/webpack.server.js"
  },
  "dependencies": {
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "express": "^4.17.3",
    "fs-extra": "^10.0.1",
    "node-fetch": "~2.6.7",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "url-parse": "^1.5.10",
    "wait-on": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.23",
    "@types/node-fetch": "^2.6.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/url-parse": "^1.4.8",
    "babel-loader": "^8.2.4",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "glob": "^7.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "ts-loader": "^9.2.8",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-node-externals": "^3.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Laffery/webpack-starter-kit"
  },
  "bugs": {
    "url": "https://github.com/Laffery/webpack-starter-kit/issues"
  },
  "homepage": "https://github.com/Laffery/webpack-starter-kit#readme"
}
