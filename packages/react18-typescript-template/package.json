{
  "name": "@laffery/webpack-react18-ts",
  "version": "0.1.0",
  "author": "Laffery",
  "description": "template for webpack + react18 + typescript",
  "private": false,
  "keywords": [
    "webpack",
    "react",
    "typescript"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development && webpack serve -c webpack/webpack.dev.js",
    "start": "cross-env NODE_ENV=production && webpack serve --port 3000 -c webpack/webpack.prod.js",
    "build": "cross-env NODE_ENV=production && rm -rf dist && webpack build -c webpack/webpack.prod.js",
    "clean:build": "rm -rf dist",
    "clean": "rm -rf dist && rm -rf node_modules",
    "test": "echo NO TEST MODULE FOUND"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "babel-loader": "^8.2.4",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "react-dev-utils": "^12.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Laffery/webpack-starter-kit"
  },
  "bugs": {
    "url": "https://github.com/Laffery/webpack-starter-kit/issues"
  },
  "homepage": "https://github.com/Laffery/webpack-starter-kit#readme"
}
